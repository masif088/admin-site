{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/src/*": ["./src/*"],
      "@/*": ["./*"],

    },
//    "paths": {
//      "@/layouts/*": ["src/layouts/*"],
//      "@/components/*": ["src/components/*"],
//      "@/public/*": ["./public/*"]
//    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/Ui-kits/Accordion/SimpleAccordion/SimpleDemo/index.tsx"
  ],
  "exclude": ["node_modules"]
}
